{
	"extends": "@repo/typescript-config/nextjs.json",
	"compilerOptions": {
		"paths": {
			"@/*": ["./*"]
		},
		"plugins": [
			{
				"name": "next"
			}
		],
		"types": ["@cloudflare/workers-types/2023-07-01"]
	},
	"include": [
		"next-env.d.ts",
		"next.config.mjs",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts"
	],
	"exclude": ["node_modules"]
}
